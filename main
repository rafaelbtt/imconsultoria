package br.imconsultoria;

import android.content.Intent;
import android.os.Bundle;
import android.view.View;

import androidx.appcompat.app.AppCompatActivity;

public class MainActivity extends AppCompatActivity {

    @Override
    protected void onCreate(Bundle savedInstanceState) {
        super.onCreate(savedInstanceState);
        setContentView(R.layout.activity_main);
    }

    public void irParaTela2(View view){
        Intent  intent1 = new Intent(getApplicationContext(), Tela2.class);
        startActivity(intent1);
    }

    public void irParaTelars(View view){
        Intent  intent2 = new Intent(getApplicationContext(), RedesSociais.class);
        startActivity(intent2);
    }

    public void irParaTelasobre(View view){
        Intent  intent3 = new Intent(getApplicationContext(), Sobre.class);
        startActivity(intent3);
    }
}
